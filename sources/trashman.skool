 $A79C LD B,$0A      ; #REGb=#N$0A.
*$A79E CALL $CB9C    ; Call #R$CB9C.
 $A7A1 LD HL,$CC3D   ; #REGhl=#R$CC3D.
 $A7A4 LD (HL),$00   ; Write #N$00 to *#REGhl.
 $A7A6 DJNZ $A79E    ; Decrease counter by one and loop back to #R$A79E until
                     ; counter is zero.
 $A7A8 LD (HL),$01   ; Write #N$01 to *#REGhl.
 $A7AA LD HL,$0080   ; {Write #N($0080,$04,$04) #REGhl to *#R$CC3B.
 $A7AD LD ($CC3B),HL ; }
 $A7B0 CALL $A6C7    ; Call #R$A6C7.
 $A7B3 POP HL        ; Restore #REGhl from the stack.
 $A7B4 CALL $B6BB    ; Call #R$B6BB.
 $A7B7 LD A,E        ; #REGa=#REGe.
