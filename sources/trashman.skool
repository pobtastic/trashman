 $A7BE JR Z,$A818    ; }
; #HTML(Was "fire" <em>not</em> pressed?)
 $A7C0 CP $80        ; {Jump to #R$A796 if #REGa is not #N$80.
 $A7C2 JR NZ,$A796   ; }
 $A7C4 PUSH HL       ; Stash #REGhl on the stack.
 $A7C5 LD HL,$0040   ; {Write #N($0040,$04,$04) to *#R$CC3B.
 $A7C8 LD ($CC3B),HL ; }
 $A7CB LD HL,$AB8A   ; {Decrease *#R$AB8A by one.
 $A7CE DEC (HL)      ; }
 $A7CF LD A,$07      ; #REGa=#N$07.
 $A7D1 AND (HL)      ; Merge the bits from *#REGhl.
 $A7D2 JR Z,$A7DF    ; Jump to #R$A7DF if the result is zero.
 $A7D4 CALL $A83A    ; Call #R$A83A.
 $A7D7 CALL $A7EB    ; Call #R$A7EB.
 $A7DA POP HL        ; Restore #REGhl from the stack.
 $A7DB INC HL        ; Increment #REGhl by one.
 $A7DC HALT          ; HALT.
 $A7DD JR $A79B      ; Jump to #R$A79B.
*$A7DF POP HL        ; Restore #REGhl from the stack.
 $A7E0 RET           ; Return.

