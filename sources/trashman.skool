 $A791 OR %00001000  ; Set bit 3.
 $A793 LD ($AB8A),A  ; Write #REGa back to #R$AB8A.
@label=Handler_NameSelect_Loop
*$A796 LD A,$00      ; {Write #N$00 to #R$CC3D.
 $A798 LD ($CC3D),A  ; }
; This entry point is used by the routine at #R$A80C.
