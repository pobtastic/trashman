; Print Numbers
;
; Used by the routines at #R$AA69, #R$AB12 and #R$CB0A.
@label=PrintNumbers
c$CAB3 LD A,$00      ; #REGa=#N$00.
 $CAB5 RLD           ; RLD.
 $CAB7 PUSH AF       ; Stash #REGaf on the stack.
 $CAB8 CP $00        ; {Jump to #R$CADA if #REGa is not #N$00.
 $CABA JR NZ,$CADA   ; }
 $CABC LD A,$20      ; #REGa=#N$20.
 $CABE CALL $CAEF    ; Call #R$CAEF.
 $CAC1 POP AF        ; Restore #REGaf from the stack.
 $CAC2 RLD           ; RLD.
 $CAC4 PUSH AF       ; Stash #REGaf on the stack.
 $CAC5 CP $00        ; {Jump to #R$CAE3 if #REGa is not #N$00.
 $CAC7 JR NZ,$CAE3   ; }
 $CAC9 LD A,$20      ; #REGa=#N$20.
 $CACB CALL $CAEF    ; Call #R$CAEF.
 $CACE POP AF        ; Restore #REGaf from the stack.
 $CACF RLD           ; RLD.
 $CAD1 DEC HL        ; Decrease #REGhl by one.
 $CAD2 DJNZ $CAB3    ; Decrease counter by one and loop back to #R$CAB3 until
                     ; counter is zero.
 $CAD4 RET           ; Return.
*$CAD5 LD A,$00      ; #REGa=#N$00.
 $CAD7 RLD           ; RLD.
 $CAD9 PUSH AF       ; Stash #REGaf on the stack.
*$CADA ADD A,$30     ; #REGa+=#N$30.
 $CADC CALL $CAEF    ; Call #R$CAEF.
 $CADF POP AF        ; Restore #REGaf from the stack.
 $CAE0 RLD           ; RLD.
 $CAE2 PUSH AF       ; Stash #REGaf on the stack.
*$CAE3 ADD A,$30     ; #REGa+=#N$30.
 $CAE5 CALL $CAEF    ; Call #R$CAEF.
 $CAE8 POP AF        ; Restore #REGaf from the stack.
 $CAE9 RLD           ; RLD.
 $CAEB DEC HL        ; Decrease #REGhl by one.
 $CAEC DJNZ $CAD5    ; Decrease counter by one and loop back to #R$CAD5 until
                     ; counter is zero.
 $CAEE RET           ; Return.
; This is similar in functionality to #R$B081.
;       It differs as it doesn't use a terminator rather the length is known.
@label=PrintNumbers_Print
*$CAEF PUSH HL       ; {Stash #REGhl and #REGbc on the stack.
 $CAF0 PUSH BC       ; }
 $CAF1 LD L,A        ; #REGl=#REGa.
 $CAF2 LD H,$00      ; #REGh=#N$00.
 $CAF4 ADD HL,HL     ; {#REGhl*=#N$08.
 $CAF5 ADD HL,HL     ;
 $CAF6 ADD HL,HL     ; }
 $CAF7 LD BC,$E417   ; #REGbc=#R$E417.
 $CAFA DEC B         ; Decrease #REGb by one.
 $CAFB ADD HL,BC     ; #REGhl+=#REGbc.
 $CAFC PUSH DE       ; Stash #REGde on the stack.
 $CAFD LD B,$08      ; #REGb=#N$08 (counter).
@label=PrintNumbers_Print_Loop
*$CAFF LD A,(HL)     ; #REGa=*#REGhl.
 $CB00 LD (DE),A     ; Write #REGa to *#REGde.
 $CB01 INC HL        ; Increment #REGhl by one.
 $CB02 INC D         ; Increment #REGd by one.
 $CB03 DJNZ $CAFF    ; Decrease counter by one and loop back to #R$CAFF until
                     ; counter is zero.
 $CB05 POP DE        ; Restore #REGde from the stack.
 $CB06 INC DE        ; Increment #REGde by one.
 $CB07 POP BC        ; {Restore #REGbc and #REGhl from the stack.
 $CB08 POP HL        ; }
 $CB09 RET           ; Return.

; Print Bonus/ Score
;
; Used by the routine at #R$B0A1.
;
; .
;
; Print the current "Bonus" value.
@label=Print_Bonus
c$CB0A LD B,$02      ; #REGb=#N$02 (counter).
 $CB0C LD DE,$403B   ; #REGde=#N$403B (screen buffer location).
 $CB0F LD HL,$C272   ; #REGhl=#R$C272.
; Work backwards.
 $CB12 INC HL        ; Increment #REGhl by one.
 $CB13 CALL $CAB3    ; Call #R$CAB3.
; Print the current score.
 $CB16 LD DE,$4020   ; #REGde=#N$4020 (screen buffer location).
; This entry point is used by the routine at #R$A8D4.
@label=Print_Score
*$CB19 LD B,$03      ; #REGb=#N$03 (counter).
 $CB1B LD HL,$CB98   ; #REGhl=#R$CB98 (last digit of #R$CB96).
 $CB1E CALL $CAB3    ; Call #R$CAB3.
 $CB21 RET           ; Return.

; Add Points To Score
;
; Used by the routines at #R$A590 and #R$B724.
;
; HL Pointer to address of points to add to score
;
; An example of #REGhl is #R$C272 which would add the time remaining to the
; score when the level is complete.
@label=AddPointsToScore
c$CB22 LD IX,$CB96    ; #REGix=#R$CB96.
; Handle the "tens" units.
 $CB26 LD A,(IX+$00)  ; #REGa=*#REGix+#N$00.
 $CB29 ADD A,(HL)     ; {Add *#REGhl to score byte #1 with BCD conversion.
 $CB2A DAA            ; }
 $CB2B LD (IX+$00),A  ; Write #REGa to *#REGix+#N$00.
; Move onto the next score digit.
 $CB2E INC HL         ; Increment #REGhl by one.
; Handle the "thousands" units.
 $CB2F LD A,(IX+$01)  ; #REGa=*#REGix+#N$01.
 $CB32 ADC A,(HL)     ; {Add (with carry) *#REGhl to score byte #2 with BCD
 $CB33 DAA            ; conversion.}
; Handle the "hundreds of thousands" units.
 $CB34 LD (IX+$01),A  ; Write #REGa to *#REGix+#N$01.
; The final digit always has a base of #N$00.
 $CB37 LD A,$00       ; {Add (with carry) #N$00 to *#REGix+#N$02 to score byte
 $CB39 ADC A,(IX+$02) ; #3.}
 $CB3C LD (IX+$02),A  ; Write #REGa to *#REGix+#N$02.
 $CB3F RET            ; Return.

; Handler: Time
;
; Used by the routine at #R$B12B.
@label=Handler_Time
c$CB40 LD HL,$C272   ; #REGhl=#R$C272.
; Fetch the small unit of time (e.g. for "250" this is "50").
 $CB43 LD A,(HL)     ; #REGa=*#REGhl.
; Take off one unit of time (see #POKE#infinite_time()).
 $CB44 SUB $01       ; {#REGa-=#N$01 with BCD conversion.
 $CB46 DAA           ; }
 $CB47 LD (HL),A     ; Write #REGa to *#REGhl.
 $CB48 RET NC        ; Return if #REGa is higher than #N$00.
; We've ticked over the small unit, so move onto the larger unit.
 $CB49 INC HL        ; Increment #REGhl by one.
; Fetch the large unit of time (e.g. for "250" this is "2").
 $CB4A LD A,(HL)     ; #REGa=*#REGhl.
; Take off one unit of time.
 $CB4B SUB $01       ; {#REGa-=#N$01 with BCD conversion.
 $CB4D DAA           ; }
 $CB4E LD (HL),A     ; Write #REGa to *#REGhl.
 $CB4F JR NZ,$CB5C   ; Jump to #R$CB5C if #REGa is not zero.
; Flash the bonus/ time countdown when the large unit is zero.
 $CB51 LD HL,$583B   ; #REGhl=#N$583B (attribute buffer address for the bonus/
                     ; time counter).
 $CB54 LD B,$04      ; #REGb=#N$04 (counter).
@label=Handler_Time_Flash
*$CB56 SET 7,(HL)    ; Set bit 7 (the flash bit) of *#REGhl.
 $CB58 INC L         ; Increment #REGl by one.
 $CB59 DJNZ $CB56    ; Decrease counter by one and loop back to #R$CB56 until
                     ; counter is zero.
 $CB5B RET           ; Return.
; Is there any time left?
@label=Handler_Time_Up
*$CB5C RET NC        ; Return if #REGa is higher than #N$00 (if there is still
                     ; time left).
; There's no time left, signify the game is over.
 $CB5D POP HL        ; {Clear the stack.
 $CB5E POP HL        ; }
 $CB5F LD HL,$B008   ; #REGhl=#R$B008.
 $CB62 SET 2,(HL)    ; Set bit 2 of *#REGhl.
 $CB64 RET           ; Return.

; Routine at CB65
;
; Used by the routine at #R$BB73.
c$CB65 PUSH IX        ; Stash #REGix on the stack.
 $CB67 LD IX,$C272    ; #REGix=#R$C272.
 $CB6B LD A,(IX+$00)  ; #REGa=*#REGix+#N$00.
 $CB6E ADD A,(HL)     ; #REGa+=*#REGhl.
 $CB6F DAA            ; DAA.
 $CB70 LD (IX+$00),A  ; Write #REGa to *#REGix+#N$00.
 $CB73 INC HL         ; Increment #REGhl by one.
 $CB74 LD A,(IX+$01)  ; #REGa=*#REGix+#N$01.
 $CB77 ADC A,(HL)     ; #REGa+=*#REGhl.
 $CB78 DAA            ; DAA.
 $CB79 LD (IX+$01),A  ; Write #REGa to *#REGix+#N$01.
 $CB7C INC HL         ; Increment #REGhl by one.
 $CB7D JR Z,$CB93     ; Jump to #R$CB93 if {} is zero.
 $CB7F LD IX,$583B    ; #REGix=#N$583B.
 $CB83 RES 7,(IX+$00) ; Reset bit 7 of *#REGix+#N$00.
 $CB87 RES 7,(IX+$01) ; Reset bit 7 of *#REGix+#N$01.
 $CB8B RES 7,(IX+$02) ; Reset bit 7 of *#REGix+#N$02.
 $CB8F RES 7,(IX+$03) ; Reset bit 7 of *#REGix+#N$03.
*$CB93 POP IX         ; Restore #REGix from the stack.
 $CB95 RET            ; Return.

