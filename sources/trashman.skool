; Print High Score Table
;
; Used by the routines at #R$A590 and #R$A62E.
@label=Print_HighScoreTable
c$AA69 CALL $A77E    ; Call #R$A77E.
 $AA6C CALL $A740    ; Call #R$A740.
; Print "TRASHMAN OF THE YEAR NOMINATIONS".
 $AA6F LD DE,$4820   ; #REGde=#N$4820 (screen buffer location).
 $AA72 LD HL,$AE07   ; #REGhl=#R$AE07.
 $AA75 CALL $B081    ; Call #R$B081.
 $AA78 LD HL,$4840   ; #REGhl=#N$4840 (screen buffer location).
 $AA7B LD B,$20      ; #REGb=#N$20.
 $AA7D CALL $A895    ; Call #R$A895.
 $AA80 LD DE,$4865   ; #REGde=#N$4865 (screen buffer location).
 $AA83 LD HL,$B043   ; #REGhl=#R$B043.
; The game holds four high scores.
 $AA86 LD B,$04      ; #REGb=#N$04 (counter).
@label=Print_HighScoreTable_Loop
*$AA88 PUSH BC       ; Stash the counter held in #REGbc on the stack.
; Print the players name for this position.
 $AA89 CALL $B081    ; Call #R$B081.
; Score are held backwards, so add three to point to the last digit.
 $AA8C LD BC,$0003   ; #REGbc=#N($0003,$04,$04).
 $AA8F ADD HL,BC     ; #REGhl+=#REGbc.
; Store the pointer for easily moving onto the next entry.
 $AA90 PUSH HL       ; Stash #REGhl on the stack.
 $AA91 EX DE,HL      ; Exchange the #REGde register with the shadow #REGhl
                     ; register.
 $AA92 ADD HL,BC     ; #REGhl+=#REGbc.
 $AA93 EX DE,HL      ; Exchange the #REGde register with the shadow #REGhl
                     ; register.
; Scores are held using three bytes.
 $AA94 LD B,$03      ; #REGb=#N$03 (counter).
 $AA96 CALL $CAB3    ; Call #R$CAB3.
; Print the "pts" suffix.
 $AA99 LD HL,$AE28   ; #REGhl=#R$AE28.
 $AA9C CALL $B081    ; Call #R$B081.
; Update the screen buffer address for the next entry.
 $AA9F LD A,E        ; {#REGe+=#N$0A.
 $AAA0 ADD A,$0A     ;
 $AAA2 LD E,A        ; }
; Restore the stashed pointer, and move onto the next entry.
 $AAA3 POP HL        ; Restore #REGhl from the stack.
 $AAA4 INC HL        ; Increment #REGhl by one.
; Fetch the counter, and loop until all the positions have been displayed.
 $AAA5 POP BC        ; Restore the counter to #REGbc from the stack.
 $AAA6 DJNZ $AA88    ; Decrease counter by one and loop back to #R$AA88 until
                     ; counter is zero.
 $AAA8 RET           ; Return.

; New High Score
;
; Used by the routines at #R$A590 and #R$A62E.
@label=NewHighScore
c$AAA9 LD IX,$B043   ; #REGix=#R$B043.
; The game stores four high scores.
 $AAAD LD B,$04      ; #REGb=#N$04.
@label=NewHighScore_CheckLoop
*$AAAF LD HL,($AB7E) ; #REGhl=*#R$AB7E.
; Move to the last digit of the active score (the hundreds of thousands).
 $AAB2 LD DE,$0004   ; {#REGhl+=#N($0004,$04,$04).
 $AAB5 ADD HL,DE     ; }
 $AAB6 LD A,(HL)     ; #REGa=*#REGhl.
 $AAB7 CP (IX+$0B)   ; {Jump to #R$AAC9 if #REGa is higher than *#REGix+#N$0B.
 $AABA JR NC,$AAC9   ; }
*$AABC LD DE,$000C   ; #REGde=#N($000C,$04,$04).
 $AABF ADD IX,DE     ; #REGix+=#REGde.
 $AAC1 DJNZ $AAAF    ; Decrease counter by one and loop back to #R$AAAF until
                     ; counter is zero.
 $AAC3 LD HL,$AB8A   ; #REGhl=#R$AB8A.
 $AAC6 RES 0,(HL)    ; Reset bit 0 of *#REGhl.
 $AAC8 RET           ; Return.
*$AAC9 JR NZ,$AADD   ; Jump to #R$AADD if {} is not zero.
 $AACB DEC HL        ; Decrease #REGhl by one.
 $AACC LD A,(HL)     ; #REGa=*#REGhl.
 $AACD CP (IX+$0A)   ; {Jump to #R$AABC if #REGa is lower than *#REGix+#N$0A.
 $AAD0 JR C,$AABC    ; }
 $AAD2 JR NZ,$AADD   ; Jump to #R$AADD if it is not zero.
 $AAD4 DEC HL        ; Decrease #REGhl by one.
 $AAD5 LD A,(HL)     ; #REGa=*#REGhl.
 $AAD6 CP (IX+$09)   ; {Jump to #R$AABC if #REGa is lower than *#REGix+#N$09.
 $AAD9 JR C,$AABC    ; }
 $AADB JR Z,$AABC    ; Jump to #R$AABC if it is zero.
*$AADD DEC B         ; Decrease #REGb by one.
 $AADE JR Z,$AAEF    ; Jump to #R$AAEF if #REGb is zero.
 $AAE0 LD A,$00      ; #REGa=#N$00.
*$AAE2 ADD A,$0C     ; #REGa+=#N$0C.
 $AAE4 DJNZ $AAE2    ; Decrease counter by one and loop back to #R$AAE2 until
                     ; counter is zero.
 $AAE6 LD C,A        ; #REGc=#REGa.
 $AAE7 LD HL,$B066   ; #REGhl=#R$B066.
 $AAEA LD DE,$B072   ; #REGde=#R$B072.
 $AAED LDDR          ; LDDR.
@label=NewHighScore_Write
*$AAEF PUSH IX       ; {#REGde=#REGix.
 $AAF1 POP DE        ; }
 $AAF2 LD HL,$AB8A   ; #REGhl=#R$AB8A.
 $AAF5 SET 0,(HL)    ; Set bit 0 of *#REGhl.
; Who is in play, is it 1UP or 2UP?
 $AAF7 BIT 4,(HL)    ; Test bit 4 of *#REGhl.
; Default to using the 1UP name.
 $AAF9 LD HL,$ACA6   ; #REGhl=#R$ACA6.
 $AAFC JR Z,$AB01    ; Jump to #R$AB01 if this is player one.
; Else, use the name for 2UP.
 $AAFE LD HL,$ACC7   ; #REGhl=#R$ACC7.
@label=NewHighScore_WriteName
*$AB01 LD BC,$0008   ; #REGbc=#N($0008,$04,$04).
 $AB04 LDIR          ; Copy the exact number of bytes of the players name to
                     ; the high score entry.
 $AB06 INC DE        ; Increment #REGde by one.
 $AB07 LD HL,($AB7E) ; #REGhl=*#R$AB7E.
 $AB0A INC HL        ; {Increment #REGhl by two.
 $AB0B INC HL        ; }
; Scores are held using three bytes.
 $AB0C LD BC,$0003   ; #REGbc=#N($0003,$04,$04).
 $AB0F LDIR          ; Copy the three score digits for the current player into
                     ; the high score entry.
 $AB11 RET           ; Return.

